        "# Prior probabilities for the classes\n",
        "def class_probability(data):\n",
        "  example_numb = len(data)\n",
        "  prob = dict(Counter(data))\n",
        "  for key in prob.keys():\n",
        "    prob[key] = prob[key] / float(example_numb)\n",
        "  return prob"
      ],
      "execution_count": 0,
      "outputs": []
    },
